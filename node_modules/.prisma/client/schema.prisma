generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model BlockNumber {
  slug      String @id
  number    Int    
  updatedAt DateTime @updatedAt
}


model Analytics {
  id      String @id @default(uuid())
  slug    String
  key_1    String
  value_1    Float
  key_2      String?
  value_2    Float?
  date String
  updatedAt DateTime @updatedAt
  createdAt DateTime  @default(now())

  @@unique([slug, date], name: "slug_date_unique")
}
